    <section id="content">
        <div class='container'>
            <div class='row' style="padding-bottom:0px;" id='content-wrapper'>
                <div class='col-xs-12 col-sm-12'>
                    <div class="page-header page-header-with-buttons">
                        <div class="row">
                            <div class="col-xs-12 col-sm-6">
                                <h1 class="pull-left">
                                    <i class="icon-home"></i>
                                    <span>编辑广告位</span>
                                </h1>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 col-sm-12">
                    <div class="box">
                        <div class="row">
                            <div class="col-xs-12 col-sm-12">
                                <div class="box">
                                    <div class="box-content">
                                        <!--<form class="form form-horizontal" style="margin-bottom: 0;" method="post"
                                              action="advertising.html" accept-charset="UTF-8"><input name="authenticity_token"
                                                                                       type="hidden">-->
																				<%= form_for @position, url: { action: :update }, html: { class: "form form-horizontal", style: "margin-bottom: 0;" } do |f| %>
                                            <div class="form-group">
                                                <div class="form-group">
<!--                                                    <label class="col-md-2 control-label" for="inputText1">名&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;称</label>-->
																										<%= f.label "名称", class: "col-md-2 control-label" %>
                                                    <div class="col-md-5">
                                                        <!--<input class="form-control" id="inputText1"
                                                               placeholder="请填写名称." type="text">-->
																												<%= f.text_field :name, class: "form-control" %>
                                                    </div>
                                                </div>
                                                <div class="form-group">
<!--                                                    <label class="col-md-2 control-label" for="inputText2">所属媒体</label>-->
																								<%= f.label :Url, class: "col-md-2 control-label" %>
                                                    <div class="col-md-5">
<!--                                                        <input class="form-control" id="inputText2"
                                                               placeholder="请填写所属媒体." type="text">-->
																												<%= f.text_field :url, class: "form-control" %>
                                                    </div>
                                                </div>
                                                <div class="form-group">
																										<%= f.label "所属媒体", class: "col-md-2 control-label" %>
                                                    <div class="col-md-5">
																												<%= f.select :publisher_id, options_for_select(Publisher.all.collect { |state| state.name }), {}, { class: "form-control" } %>
                                                    </div>
                                                </div>
                                                <div class="form-group">
																										<%= f.label "所属频道", class: "col-md-2 control-label" %>
                                                    <div class="col-md-5">
																												<%= f.select :channel_id, options_for_select(Channel.all.collect { |state| state.name }), {}, { class: "form-control" } %>
                                                    </div>
                                                </div>                                               <div class="form-group">
<!--                                                    <label class="col-md-2 control-label" for="inputSelect4">状&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;态</label>-->
																								<%= f.label :Status, class: "col-md-2 control-label" %>

                                                    <div class="col-md-5">
<!--                                                        <select class="form-control" id="inputSelect4">
                                                            <option>活动</option>
                                                            <option>停用</option>
                                                        </select>-->
																												<%= f.select :state_id, options_for_select(State.all.collect { |state| state.name }), {}, { class: "form-control" } %>
                                                    </div>
                                                </div>
                                                <div class="form-group">
<!--                                                    <label class="col-md-2 control-label"
                                                           for="inputSelect5">展现形式</label>-->
																								<%= f.label "广告类型", class: "col-md-2 control-label" %>
                                                    <div class="col-md-5">
<!--                                                        <select class="form-control" id="inputSelect5">
                                                            <option>通栏</option>
                                                            <option>文字链</option>
                                                            <option>对联</option>
                                                            <option>全屏</option>
                                                        </select>-->
																												<%= f.select :adtype_id, options_for_select(AdType.all.collect { |state| state.name }), {}, { class: "form-control" } %>
                                                    </div>
                                                </div>
                                                <div class="form-group">
																								<%= f.label "物料类型", class: "col-md-2 control-label" %>
                                                  <div class="col-md-5">
																										<%= f.select :material_id, options_for_select(Material.all.collect { |state| state.name }), {}, { class: "form-control" } %>
                                                	</div>
                                                </div>
																								<div class="form-group">
																								<%= f.label "展现形式", class: "col-md-2 control-label" %>
                                                  <div class="col-md-5">
																										<%= f.select :layout_id, options_for_select(Layout.all.collect { |state| state.name }), {}, { class: "form-control" } %>
                                                	</div>
                                                </div>
																								<div class="form-group">
																								<%= f.label "格式", class: "col-md-2 control-label" %>
                                                  <div class="col-md-5">
																										<%= f.select :format_id, options_for_select(Format.all.collect { |state| state.name }), {}, { class: "form-control" } %>
                                                	</div>
                                                </div>
																								<div class="form-group">
																								<%= f.label "尺寸", class: "col-md-2 control-label" %>
                                                  <div class="col-md-5">
																										<%= f.select :dimension_id, options_for_select(Dimension.all.collect { |state| state.name }), {}, { class: "form-control" } %>
                                                	</div>
                                                </div>
																								<div class="form-group">
																								<%= f.label "大小", class: "col-md-2 control-label" %>
                                                  <div class="col-md-5">
																										<%= f.text_field :size, class: "form-control" %>
                                                	</div>
                                                </div>
																								<div class="form-group">
																								<%= f.label "投放形式", class: "col-md-2 control-label" %>
                                                  <div class="col-md-5">
																										<%= f.select :serving_id, options_for_select(ServingType.all.collect { |state| state.name }), {}, { class: "form-control" } %>
                                                	</div>
                                                </div>
																								<div class="form-group">
																								<%= f.label "付费方式", class: "col-md-2 control-label" %>
                                                  <div class="col-md-5">
																										<%= f.select :payment_id, options_for_select(PaymentType.all.collect { |state| state.name }), {}, { class: "form-control" } %>
                                                	</div>
                                                </div>
                                                <div class="form-actions form-actions-padding-sm">
                                                    <div class="row">
                                                        <div class="col-md-10 col-md-offset-2">
<!--                                                            <button class="btn btn-primary" type="submit">
                                                                <i class="icon-save"></i>
                                                                Save
                                                            </button>
                                                            <button class="btn" type="submit">Cancel</button>-->
																														<%= f.submit "Save", class: "btn btn-primary" %>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        <% end %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
<!-- your & Flatty javascripts -->
<!-- / jquery [required] -->
<script src="/assets/jquery/jquery.min.js" type="text/javascript"></script>
<!-- / jquery mobile (for touch events) -->
<script src="/assets/jquery/jquery.mobile.custom.min.js" type="text/javascript"></script>
<!-- / jquery migrate (for compatibility with new jquery) [required] -->
<script src="/assets/jquery/jquery-migrate.min.js" type="text/javascript"></script>
<!-- / jquery ui -->
<script src="/assets/jquery/jquery-ui.min.js" type="text/javascript"></script>
<!-- / jQuery UI Touch Punch -->
<script src="/assets/plugins/jquery_ui_touch_punch/jquery.ui.touch-punch.min.js"
        type="text/javascript"></script>
<!-- / bootstrap [required] -->
<script src="/assets/bootstrap/bootstrap.js" type="text/javascript"></script>
<!-- / modernizr -->
<script src="/assets/plugins/modernizr/modernizr.min.js" type="text/javascript"></script>
<!-- / retina -->
<script src="/assets/plugins/retina/retina.js" type="text/javascript"></script>
<!-- / theme file [required] -->
<script src="/assets/theme.js" type="text/javascript"></script>

<script src="/assets/plugins/common/moment.min.js" type="text/javascript"></script>
<script src="/assets/plugins/bootstrap_daterangepicker/bootstrap-daterangepicker.js"
        type="text/javascript"></script>

<!-- project request -->
<script type="text/javascript" src="/assets/project.js"></script>

<script>
    $(function () {
        $("#cancel").click(function () {
            window.location = "adSpace.html";
        });
    });
</script>

</body>
</html>
